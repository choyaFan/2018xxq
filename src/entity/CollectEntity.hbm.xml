<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.CollectEntity" table="collect" schema="javademo">
        <id name="collectId">
            <column name="collect_id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="goodsId">
            <column name="goods_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>